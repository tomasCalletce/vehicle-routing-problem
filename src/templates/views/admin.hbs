    <div class="container ">
        <div class="d-flex align-content-center justify-content-center">
            <div class="container w-75 ">

                <div class="row bg-light rounded mt-4">
                    <div class="col-12">
                        <h3>crear computador</h3>

                        <div class="form-group">
                            <label for="exampleFormControlSelect1">computador id</label>
                            <input class="form-control" type="number" placeholder="" id="idComputador">

                            <label for="exampleFormControlSelect1">codigo aula</label>
                            <input class="form-control" type="number" placeholder="" id="codigoAula">

                            <label for="exampleFormControlSelect1">CPU</label>
                            <input class="form-control" type="text" placeholder="" id="tipoCPU">

                            <label for="exampleFormControlSelect1">GPU</label>
                            <input class="form-control" type="text" placeholder="" id="GPU">

                            <label for="exampleFormControlSelect1">RAM GB</label>
                            <input class="form-control" type="number" placeholder="" id="GBRAM">

                        </div>
                        <button type="submit" class="btn btn-primary mb-2" id="crearComputador">guardar</button>

                    </div>
                </div>

                <div class="row  mt-4">
                    <div class="col-12 bg-light rounded">
                        <h3>crear aula</h3>
                        <div class="form-group">
                            <label for="exampleFormControlSelect1">codigo aula</label>
                            <input class="form-control" type="text" placeholder="" id="codigoCrearAula">

                            <label for="exampleFormControlSelect1">cupo maximo</label>
                            <input class="form-control" type="number" placeholder="" id="cupoMaximo">

                            <label for="exampleFormControlSelect1">id asistente tecnico</label>
                            <input class="form-control" type="text" placeholder="" id="idAsistente">

                        </div>
                        <button type="submit" class="btn btn-primary mb-2" id="crearAula">guardar</button>

                    </div>
                </div>

                <div class="row bg-light rounded mt-4">
                    <div class="col-12">
                        <h3>crear asistente tecnico</h3>

                        <div class="form-group">
                            <label for="exampleFormControlSelect1">asistente id</label>
                            <input class="form-control" type="number" placeholder="" id="asistente">

                            <label for="exampleFormControlSelect1">nombre</label>
                            <input class="form-control" type="text" placeholder="" id="nombreAsistente">

                            <label for="exampleFormControlSelect1">correo</label>
                            <input class="form-control" type="email" placeholder="" id="correoAsistente">

                            <label for="exampleFormControlSelect1">celular</label>
                            <input class="form-control" type="text" placeholder="" id="celularAsistente">

                        </div>
                        <button type="submit" class="btn btn-primary mb-2" id="crearAsistenteTecnico">guardar</button>

                    </div>
                </div>

            </div>
        </div>
